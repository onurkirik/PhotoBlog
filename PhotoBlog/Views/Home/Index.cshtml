@model List<Post>
@{
    ViewData["Title"] = "Home Page";
}

@foreach(var item in Model){
    <div class="post">
        <div>
            <h2>@item.Title</h2>
            <img src="~/img/@item.Photo" alt="@item.Title" />
        </div>
        <div>
            Posted on: @item.CreatedTime
        </div>
        <p>@item.Description</p>
    </div>
}